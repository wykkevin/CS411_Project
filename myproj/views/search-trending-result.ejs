<!DOCTYPE html>
<html>
<head>
  <title>Search Results</title>
</head>
<body>
  <h1>Search Trending Results</h1>
  <% if (results.length === 0) { %>
    <p>No results found for <%= videoTitle %>.</p>
  <% } else { %>
    <table>
  <thead>
    <tr>
      <th>Video Thumbnail</th>
      <th>Video Id</th>
      <th>Trending Date</th>
      <th>Video Title</th>
      <th>Channel Title</th>
      <th>View Count</th>
      <th>Like ratio</th>
      <th>Likes</th>
      <th>Comment Count</th>
    </tr>
  </thead>
  <tbody>
    <% results.forEach((result) => { %>
      <tr>
        <td><img src="<%= result.thumbnail_link %>"></td>
        <td><%= result.videoId %></td>
        <td><%= result.date %></td>
        <td><%= result.title %></td>
        <td><%= result.channelTitle %></td>
        <td><%= result.view_count %></td>
        <td><%= result.like_ratio %></td>
        <td><%= result.likes %></td>
        <td><%= result.comment_count %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
  <% } %>
</body>
</html>
