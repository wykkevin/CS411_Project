<!DOCTYPE html>
<html>
<head>
  <title>100 Most Viewed Videos</title>
</head>
<body>
  <h1>100 Most Viewed Videos</h1>
  <% if (results.length === 0) { %>
    <p>No results found.</p>
  <% } else { %>
    <table>
  <thead>
    <tr>
	<th>Video Image</th>
      <th>Video Title</th>
      <th>Number of Views</th>
    </tr>
  </thead>
  <tbody>
    <% results.forEach((result) => { %>
      <tr>
<td><img src="<%= result.thumbnail_link %>"></td>
        <td><%= result.title %></td>
        <td><%= result.videoViewCount %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
  <% } %>
</body>
</html>
