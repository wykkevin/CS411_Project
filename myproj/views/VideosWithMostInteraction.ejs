<!DOCTYPE html>
<html>
<head>
  <title>Videos With Most Interaction</title>
</head>
<body>
  <h1>Videos With Most Interaction</h1>
  <% if (results.length === 0) { %>
    <p>No results found.</p>
  <% } else { %>
    <table>
  <thead>
    <tr>
      <th>Video Image</th>
      <th>Video Title</th>
      <th>Number of Views</th>
      <th>Number of Likes</th>
      <th>Number of Dislikes</th>
      <th>Number of Comments</th>
      <th>Like/Dislike Ratio</th>
    </tr>
  </thead>
  <tbody>
    <% results.forEach((result) => { %>
      <tr>
	<td><img src="<%= result.thumbnail_link %>"></td>
        <td><%= result.title %></td>
        <td><%= result.videoViewCount %></td>
        <td><%= result.videoLikeCount %></td>
        <td><%= result.videoDislikeCount %></td>
        <td><%= result.commentCount %></td>
        <td><%= result.likeRatio %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
  <% } %>
</body>
</html>
